#include <stdio.h>
int main()
{
int num;
printf("enter a number to print its multiplication table:\n");
scanf("%d", &num);

printf("%d x 1 \n", num, num*1);
printf("%d x 2 \n", num, num*1);
printf("%d x 3 \n", num, num*1);
printf("%d x 4 \n", num, num*1);
printf("%d x 5\n", num, num*1);
printf("%d x 6 \n", num, num*1);
printf("%d x 7 \n", num, num*1);
printf("%d x 8 \n", num, num*1);
printf("%d x 9 \n", num, num*1);
printf("%d x 10 \n", num, num*1);
return 0;
}
